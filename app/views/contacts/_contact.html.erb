
<% c_id = contact.id %>

<h3 class="w3-large">
	<i class="material-icons w3-large" id='<%= contact.id %>' data-id='<%= c_id %>'>keyboard_arrow_down</i>
	<%= link_to "#{contact.first_name} #{contact.last_name} - #{contact.workplace}", user_contact_path(contact.user, contact) %>
</h3>

<span id='<%= contact.id %>' class='remove <%= contact.id %>' style="display: none;" >Remove</span>




